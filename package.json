  "devDependencies": {
    "@netlify/plugin-functions-install-core": "^2.0.0",
    "@netlify/plugin-lighthouse": "^5.0.0",
    "@netlify/plugin-sitemap": "^0.12.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
